{
  "name": "server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env nodemon --ext ts,json --exec babel-node --extensions '.ts' src/index.ts",
    "test": "yarn lint && yarn tsc",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "build": "yarn build:clean && yarn build:babel",
    "build:babel": "babel src --out-dir build --copy-files --extensions='.ts'",
    "build:clean": "rm -rf build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  }
}
